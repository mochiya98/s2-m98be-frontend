<!DOCTYPE html><html lang="ja"><head><meta charset="UTF-8">
<title><%= htmlWebpackPlugin.options.title %></title>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons&display=block" rel="stylesheet">
<% if (Array.isArray(htmlWebpackPlugin.options.meta)) { %>
  <% for (item of htmlWebpackPlugin.options.meta) { %>
<meta<% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>>
  <% } %>
<% } %>
<% for (key in htmlWebpackPlugin.files.css) { %>
    <link href="<%= htmlWebpackPlugin.files.css[key] %>" rel="stylesheet" media="nope!" onload="this.media='all'">
<% } %>
</head><body>
 
<% for (key in htmlWebpackPlugin.options.modernScripts) { %>
  <script type="module" src="<%= htmlWebpackPlugin.options.modernScripts[key] %>"></script>
<% } %>
  <script nomodule><% for (key in htmlWebpackPlugin.options.legacyScripts) { %>document.body.appendChild(document.createElement('script')).src='<%= htmlWebpackPlugin.options.legacyScripts[key] %>';<% } %></script>
</body></html>